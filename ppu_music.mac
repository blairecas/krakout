	beq	PD_REL  
	bhi	PD_VOL
	cmpb	R0, #260			; 0xB0
	beq	PD_EOFF  
	bhi	PD_SORE  
  
	cmpb	R0, #120			; 0x50
	bhis	PD_NOTE  
	cmpb	R0, #100			; 0x40
	bhis	PD_ORN  
	cmpb	R0, #40				; 0x20
	bhis	PD_NOIS 
	cmpb	R0, #20				; 0x10
	bhis	PD_ESAM 

	asl	R0
	mov	SPEC_SUBR(R0), -(SP) 
	br	PD_LOOP

PD_NOIS:
	bic	#177740, R0			; 0xFFE0
	movb	R0, PARAM_NOISE_BASE(R4)
	br	PD_LOOP

PD_REL:
	clrb	CHP_ENABLED(R5)
	br	PD_RES

PD_NOTE:	
	sub	#120, R0			; 0x50
	mov	R0, CHP_NOTE(R5)
	movb	#-1, CHP_ENABLED(R5)

