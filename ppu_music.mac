	mov	R4, CUR_PARAMS_ADDR
	clr	PARAM_AYREGS + AY_AMPLITUDEA(R4)
	clrb	PARAM_AYREGS + AY_AMPLITUDEC(R4)
	movb	#77, PARAM_AYREGS + AY_MIXER(R4)
	br	REG_OUT

