	bcs	14$  
	cmpb	R0, #10.
	blo	15$
14$:	mov	#6, R0   
15$: 	mov	CUR_PARAMS_ADDR, R2
	movb	R0, PARAM_VERSION(R2) 
	mov	R0, -(SP)   
	cmpb	R0, #4   
	movb	99.(R5), R0
	rolb	R0
	bicb	#177770, R0

