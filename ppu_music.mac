	clr	R3
	bisb	(R1), R3
	add	#TABLES, R3
	add	(SP)+, R2   

	mov	R3, -(SP)
	mov	#PARAM_NOTE_TAB, R1 
	add	CUR_PARAMS_ADDR, R1
