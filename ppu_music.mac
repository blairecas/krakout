	mov	R1, -(SP)   
	mov	#NT_DATA, R1
	add	R0, R1
	add	R0, R1
	add	R0, R1  
	clr	R2
	bisb	(R1)+, R2   
	mov	(PC)+, R0
	nop	
	tstb	(R1)+   
	beq	10$
	mov	(PC)+, R0   
 	clc	
